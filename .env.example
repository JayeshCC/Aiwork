# AIWork Configuration Template
# Copy this to .env and fill in your values

# API Configuration
AIWORK_API_HOST=0.0.0.0
AIWORK_API_PORT=8000

# Kafka Configuration
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_TASKS_TOPIC=aiwork-tasks
KAFKA_RESULTS_TOPIC=aiwork-results

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# OpenVINO Configuration
OPENVINO_MODEL_DIR=models/
OPENVINO_DEVICE=CPU  # Options: CPU, GPU, MYRIAD

# Logging
LOG_LEVEL=INFO  # Options: DEBUG, INFO, WARNING, ERROR
